{
  "swagger": "2.0",
  "info": {
    "description": "To search for person, tenure or asset information",
    "version": "1.0.0",
    "title": "Housing Search API"
  },
  "host": "virtserver.swaggerhub.com",
  "basePath": "/Hackney/housingSearchApi/1.0.0",
  "tags": [
    {
      "name": "GetAssetList"
    },
    {
      "name": "GetPersonList"
    },
    {
      "name": "GetTenureList"
    },
    {
      "name": "GetTransactionsList"
    }
  ],
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/search/persons": {
      "get": {
        "tags": [
          "GetPersonList"
        ],
        "description": "Retrieve all person information through search",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "searchText",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": false,
            "type": "integer"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer"
          },
          {
            "name": "sortBy",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "isDesc",
            "in": "query",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved person details upon request",
            "schema": {
              "$ref": "#/definitions/personResult"
            }
          },
          "404": {
            "description": "Person not found"
          }
        }
      }
    },
    "/search/tenures": {
      "get": {
        "tags": [
          "GetTenureList"
        ],
        "description": "Retrieve all tenure information through search",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "searchText",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": false,
            "type": "integer"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer"
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved tenure details upon request",
            "schema": {
              "$ref": "#/definitions/tenureResult"
            }
          },
          "404": {
            "description": "Tenure not found"
          }
        }
      }
    },
    "/search/assets": {
      "get": {
        "tags": [
          "GetAssetList"
        ],
        "description": "Retrieve all asset information through search",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "searchText",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": false,
            "type": "integer"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer"
          },
          {
            "name": "assetTypes",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "LettableNonDwelling",
              "Dwelling",
              "Estate",
              "..."
            ]
          },
          {
            "name": "propertyReference",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved asset details upon request",
            "schema": {
              "$ref": "#/definitions/assetResult"
            }
          },
          "404": {
            "description": "Asset not found"
          }
        }
      }
    },
    "/search/transactions": {
      "get": {
        "tags": [
          "GetTransactionsList"
        ],
        "description": "Paginated search for transactions based on searchText",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "searchText",
            "in": "query",
            "description": "Some search phrase. one of the searchText or the targetid should'nt be null or empty",
            "required": false,
            "type": "string"
          },
          {
            "name": "targetId",
            "in": "query",
            "description": "Some search phrase. one of the searchText or the targetid should'nt be null or empty",
            "required": false,
            "type": "string",
            "format": "Guid"
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "Page size. Default value is 12",
            "required": false,
            "type": "integer"
          },
          {
            "name": "page",
            "in": "query",
            "description": "Page number for pagination",
            "required": false,
            "type": "integer"
          },
          {
            "name": "startDate",
            "in": "query",
            "description": "Start date of the period to load transactions",
            "required": false,
            "type": "string",
            "format": "date"
          },
          {
            "name": "endDate",
            "in": "query",
            "description": "End date of the period to load transactions",
            "required": false,
            "type": "string",
            "format": "date"
          },
          {
            "name": "isSuspense",
            "in": "query",
            "description": "Flag that determines whether to return only suspense transactions",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved asset details upon request",
            "schema": {
              "$ref": "#/definitions/transactionsResponse"
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  },
